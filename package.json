{
    "name": "react-material-design",
    "version": "0.1.9",
    "description": "react material-components",
    "main": "index.js",
    "contributors": [
        {
            "email": "miguel@reactmaterial.com",
            "name": "Miguel Amaro",
            "url": "https://github.com/miguel-orange"
        }
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/react-material-design/react-material-design.git"
    },
    "bugs": {
        "url": "https://github.com/react-material-design/react-material-design/issues"
    },
    "homepage": "https://reactmaterial.com",
    "keywords": [
        "google design",
        "google material design",
        "material components",
        "material design",
        "material",
        "material-component",
        "material-component-web",
        "material.io",
        "react",
        "reactjs",
        "webpack"
    ],
    "scripts": {
        "build": "webpack",
        "build:min": "cross-env NODE_ENV=production webpack",
        "cleanup": "rimraf '{build,coverage}' --rmdir && rimraf react-material-design-*.tgz",
        "dist": "yarn cleanup && yarn build && yarn build:min",
        "docgen": "react-docgen ./components --pretty -o output.json -e component.js -e react-material-design.js -i classes",
        "lint": "eslint components --ext .js",
        "prepublish": "yarn dist",
        "test": "jest",
        "test:coverage": "jest --coverage"
    },
    "dependencies": {
        "babel-cli": "^6.24.1",
        "babel-core": "^6.24.1",
        "babel-eslint": "^7.2.3",
        "babel-loader": "^7.0.0",
        "babel-plugin-add-module-exports": "^0.2.1",
        "classnames": "^2.2.5",
        "cross-env": "^5.0.0",
        "css-loader": "^0.28.1",
        "eslint-loader": "^1.6.1",
        "immutable": "^3.8.1",
        "material-components-web": "^0.10.0",
        "prop-types": "^15.5.10",
        "raw-loader": "^0.5.1",
        "react": "^15.5.4",
        "react-docgen": "^2.15.0",
        "react-dom": "^15.5.4",
        "react-router-dom": "^4.1.1",
        "rimraf": "^2.6.1",
        "style-loader": "^0.17.0",
        "uuid": "^3.0.1",
        "webpack": "^2.5.1"
    },
    "devDependencies": {
        "babel-jest": "^20.0.1",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-node6": "^11.0.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "enzyme": "^2.8.2",
        "enzyme-to-json": "^1.4.5",
        "eslint": "^3.15.0",
        "eslint-config-airbnb": "^15.0.1",
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-jest": "^20.0.1",
        "eslint-plugin-jsx-a11y": "^5.0.1",
        "eslint-plugin-react": "^7.0.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^20.0.1",
        "jest-enzyme": "^3.1.1",
        "react-addons-test-utils": "^15.5.1",
        "react-test-renderer": "^15.5.4"
    },
    "jest": {
        "setupTestFrameworkScriptFile": "<rootDir>/node_modules/jest-enzyme/lib/index.js",
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "transformIgnorePatterns": [
            "node_modules/?!(@material)"
        ],
        "moduleNameMapper": {
            "\\.(css|less)$": "identity-obj-proxy"
        }
    }
}
