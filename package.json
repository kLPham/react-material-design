{
  "name": "react-material-design",
  "version": "0.3.3",
  "description": "react material-components",
  "main": "index.js",
  "contributors": [
    {
      "email": "miguel@reactmaterial.com",
      "name": "Miguel Amaro",
      "url": "https://github.com/miguel-orange"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-material-design/react-material-design.git"
  },
  "bugs": {
    "url": "https://github.com/react-material-design/react-material-design/issues"
  },
  "homepage": "http://reactmaterial.com",
  "keywords": [
    "google design",
    "google material design",
    "material components",
    "material design",
    "material",
    "material-component",
    "material-component-web",
    "material.io",
    "react",
    "reactjs",
    "webpack"
  ],
  "scripts": {
    "build": "webpack",
    "build:min": "cross-env NODE_ENV=production webpack",
    "cleanup": "rimraf '{build,coverage}' --rmdir && rimraf react-material-design-*.tgz",
    "dist": "yarn cleanup && yarn build && yarn build:min",
    "docgen": "react-docgen ./components -o output.json -e component.js -e react-material-design.js --ignore classes",
    "lint": "eslint components --ext .js",
    "prepare": "yarn dist",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "material-components-web": "^0.16.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.2",
    "react-test-renderer": "^15.6.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.6.0",
    "react-addons-test-utils": "^15.6.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/node_modules/jest-enzyme/lib/index.js",
    "transformIgnorePatterns": [
      "node_modules/?!(@material)"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
